{
  "presets": [
    "@babel/preset-env",
    "preact"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    [
      "module-resolver",
      {
        "root": [
          "."
        ],
        "alias": {
          "react": "preact-compat",
          "react-dom": "preact-compat"
        }
      }
    ]
  ],
  "env": {
    "legacy": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": "> 1%, not dead, ie 9",
            "modules": false,
            "useBuiltIns": "usage",
            "forceAllTransforms": true,
            "ignoreBrowserslistConfig": true
          }
        ],
      ],
      "plugins": ["@babel/plugin-transform-runtime"]
    }
  }
}