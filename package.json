{
  "name": "mumblebot",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "get-stations": "node ./util/get-stations.js",
    "build": "npm-run-all --parallel build:*",
    "build:client": "cross-env BABEL_ENV=client parcel build --detailed-report --experimental-scope-hoisting ./client/index.js",
    "build:legacy": "cross-env BABEL_ENV=legacy parcel build --detailed-report ./client/legacy.js",
    "build:server": "cross-env BABEL_ENV=server parcel build --detailed-report --target node  ./server/main.js ./server/mumble/mumble.js",
    "dev": "npm-run-all --parallel client server start",
    "lint": "eslint . --fix",
    "start": "cross-env BABEL_ENV=server nodemon --ignore server/ --ignore stations.json --ignore client/ ./dist/main.js",
    "client": "cross-env BABEL_ENV=client parcel watch client/index.js",
    "server": "cross-env BABEL_ENV=server parcel watch  --target node ./server/main.js ./server/mumble/mumble.js"
  },
  "browserslist": ">1%, not ie 11, not op_mini all",
  "alias": {
    "react": "preact-compat",
    "react-dom": "preact-compat"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@babel/runtime": "^7.2.0",
    "audio-metadata": "^0.3.0",
    "audio-mixer": "https://github.com/notthetup/audio-mixer.git",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "express": "^4.16.4",
    "fast-sort": "^1.5.4",
    "fluent-ffmpeg": "^2.1.2",
    "hbs": "^4.0.1",
    "is-safari": "^1.0.0",
    "isomorphic-unfetch": "^3.0.0",
    "linkstate": "^1.1.1",
    "lodash.clonedeep": "^4.5.0",
    "module-alias": "^2.1.0",
    "mongodb": "^3.1.10",
    "multer": "^1.4.1",
    "mumble": "^0.3.19",
    "navaid": "^1.0.0",
    "node-schedule": "^1.3.1",
    "preact": "^8.4.2",
    "preact-compat": "^3.18.2",
    "preact-virtual-list": "^0.3.1",
    "promise-memoize": "^1.2.1",
    "react-keydown": "^1.9.7",
    "regenerator-runtime": "^0.13.1",
    "styled-components": "^3.4.2",
    "threads": "^0.12.0",
    "underscore": "^1.9.1",
    "unistore": "^3.1.0",
    "universal-cookie": "^3.0.7",
    "ytdl-core": "^0.28.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/register": "^7.0.0",
    "babel-node-modules": "^0.0.1",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-preact": "https://github.com/tomasdev/babel-preset-preact.git#patch-1",
    "cross-env": "^5.2.0",
    "eslint": "5.11.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-preact": "^1.1.6",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.0",
    "eslint-plugin-standard": "^4.0.0",
    "nodemon": "^1.18.9",
    "npm-run-all": "^4.1.5",
    "parcel": "^1.11.0",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "preact-render-to-string": "^4.1.0"
  }
}
