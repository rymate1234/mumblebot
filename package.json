{
  "name": "mumblebot",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "get-stations": "node ./util/get-stations.js",
    "build": "npm-run-all --parallel build:*",
    "build:client": "cross-env BABEL_ENV=client parcel build --detailed-report  --cache-dir .cache/client --target browser ./client/index.tsx",
    "build:server": "cross-env BABEL_ENV=server parcel build --detailed-report  --cache-dir .cache/server --target node  ./server/main.tsx ./server/mumble/index.ts",
    "dev": "npm-run-all --parallel client server start",
    "lint": "eslint . --fix",
    "start": "cross-env BABEL_ENV=server nodemon --ignore server/ --ignore stations.json --ignore client/ ./dist/main.js",
    "client": "cross-env BABEL_ENV=client parcel watch --cache-dir .cache/client --target browser client/index.tsx",
    "server": "cross-env BABEL_ENV=server parcel watch --cache-dir .cache/server --target node ./server/main.tsx ./server/mumble/index.ts"
  },
  "browserslist": ">1%, not ie 11, not op_mini all",
  "alias": {
    "react": "preact/compat",
    "react-dom": "preact/compat"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "audio-metadata": "^0.3.0",
    "audio-mixer": "https://github.com/notthetup/audio-mixer.git",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "connect-mongodb-session": "^2.2.0",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "fast-sort": "^1.5.6",
    "fluent-ffmpeg": "^2.1.2",
    "hat": "^0.0.3",
    "hbs": "^4.0.4",
    "is-safari": "^1.0.0",
    "isomorphic-unfetch": "^3.0.0",
    "linkstate": "^1.1.1",
    "lodash.clonedeep": "^4.5.0",
    "module-alias": "^2.2.1",
    "mongodb": "^3.3.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "mumble": "^0.3.21",
    "music-metadata": "^4.3.0",
    "navaid": "^1.0.5",
    "node-schedule": "^1.3.2",
    "preact": "^10.0.0-rc.1",
    "preact-virtual-list": "^0.3.1",
    "promise-memoize": "^1.2.1",
    "react-keydown": "^1.9.11",
    "react-virtual-list": "^2.3.0",
    "react-virtualized": "^9.21.1",
    "regenerator-runtime": "^0.13.3",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "source-map-support": "^0.5.13",
    "styled-components": "^4.3.2",
    "threads": "^0.12.0",
    "underscore": "^1.9.1",
    "unistore": "^3.4.1",
    "universal-cookie": "^4.0.2",
    "ytdl-core": "^0.29.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "@types/compression": "^1.0.0",
    "@types/connect-mongodb-session": "^0.0.1",
    "@types/express": "^4.17.0",
    "@types/express-session": "^1.15.13",
    "@types/morgan": "^1.7.36",
    "@types/socket.io": "^2.1.2",
    "@types/socket.io-client": "^1.4.32",
    "@types/styled-components": "^4.1.18",
    "babel-node-modules": "^0.0.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-preset-preact": "https://github.com/tomasdev/babel-preset-preact.git#patch-1",
    "cross-env": "^5.2.0",
    "eslint": "6.1.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-config-standard-preact": "^1.1.6",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-standard": "^4.0.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "parcel": "^1.12.3",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "preact-render-to-string": "^5.0.6",
    "typescript": "^3.5.3"
  }
}
