{
  "name": "mumblebot",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "get-stations": "node ./util/get-stations.js",
    "build": "cross-env BABEL_ENV=client parcel build --detailed-report  --cache-dir .cache/client --target browser ./client/index.js",
    "dev": "npm-run-all --parallel start watch",
    "lint": "eslint . --fix",
    "start": "cross-env BABEL_ENV=server nodemon --ignore stations.json --ignore client/ --exec babel-node --extensions '.js,.ts,.tsx' --tls-min-v1.0  ./server/main.js",
    "prod": "cross-env BABEL_ENV=server babel-node --extensions '.js,.ts,.tsx' --tls-min-v1.0  ./server/main.js",
    "watch": "cross-env BABEL_ENV=client parcel watch --cache-dir .cache/client --target browser client/index.js"
  },
  "browserslist": ">1%, not ie 11, not op_mini all",
  "alias": {
    "react": "preact/compat",
    "react-dom": "preact/compat"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "audio-metadata": "^0.3.0",
    "audio-mixer": "^2.1.4",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "fast-sort": "^2.2.0",
    "fluent-ffmpeg": "^2.1.2",
    "hbs": "^4.1.1",
    "is-safari": "^1.0.0",
    "isomorphic-unfetch": "^3.1.0",
    "js-yaml": "^3.14.0",
    "linkstate": "^2.0.0",
    "lodash.clonedeep": "^4.5.0",
    "module-alias": "^2.2.2",
    "mongodb": "^3.6.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "mumble": "https://github.com/minghan9456/node12-mumble",
    "music-metadata": "^7.5.0",
    "navaid": "^1.2.0",
    "node-schedule": "^1.3.2",
    "observable-fns": "^0.5.1",
    "preact": "^10.5.7",
    "preact-virtual-list": "^0.3.1",
    "promise-memoize": "^1.2.1",
    "react-keydown": "^1.9.12",
    "regenerator-runtime": "^0.13.7",
    "socket.io": "^3.0.1",
    "socket.io-client": "^3.0.1",
    "source-map-support": "^0.5.19",
    "styled-components": "^5.2.1",
    "threads": "^1.6.3",
    "tiny-worker": "^2.3.0",
    "ts-node": "^9.0.0",
    "underscore": "^1.11.0",
    "unistore": "^3.5.2",
    "universal-cookie": "^4.0.4",
    "ytdl-core": "^4.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.17",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-transform-modules-commonjs": "^7.12.13",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.17",
    "@babel/register": "^7.12.1",
    "@types/fluent-ffmpeg": "^2.1.16",
    "@types/mongodb": "^3.5.33",
    "babel-node-modules": "^0.0.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-preset-preact": "https://github.com/tomasdev/babel-preset-preact.git#patch-1",
    "cross-env": "^7.0.2",
    "eslint": "7.13.0",
    "eslint-config-standard": "^16.0.1",
    "eslint-config-standard-preact": "^1.1.6",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-standard": "^4.1.0",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "parcel": "^1.12.4",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "preact-render-to-string": "^5.1.12",
    "typescript": "^4.0.5"
  }
}
