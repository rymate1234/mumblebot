{
  "name": "mumblebot",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "get-stations": "node ./util/get-stations.js",
    "build": "npm-run-all --parallel build:client build:server",
    "build:client": "cross-env BABEL_ENV=client parcel build --detailed-report ./client/index.js",
    "build:server": "cross-env BABEL_ENV=server parcel build --detailed-report --target node ./server/main.js",
    "dev": "npm-run-all --parallel start client",
    "lint": "eslint . --fix",
    "start": "cross-env BABEL_ENV=server nodemon --ignore dist/* --ignore stations.json --ignore client/ --exec babel-node server/main.js",
    "client": "cross-env BABEL_ENV=client parcel watch client/index.js"
  },
  "alias": {
    "react": "preact-compat",
    "react-dom": "preact-compat"
  },
  "dependencies": {
    "audio-metadata": "^0.3.0",
    "audio-mixer": "https://github.com/notthetup/audio-mixer.git",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "express": "^4.16.3",
    "fast-sort": "^1.5.4",
    "fluent-ffmpeg": "^2.1.2",
    "hbs": "^4.0.1",
    "is-safari": "^1.0.0",
    "isomorphic-fetch": "^2.2.1",
    "linkstate": "^1.1.1",
    "lodash.clonedeep": "^4.5.0",
    "module-alias": "^2.1.0",
    "mongodb": "^3.1.10",
    "multer": "^1.4.1",
    "mumble": "^0.3.19",
    "navaid": "^1.0.0",
    "node-schedule": "^1.3.0",
    "preact": "^8.3.0",
    "preact-compat": "^3.18.2",
    "preact-virtual-list": "^0.3.1",
    "promise-memoize": "^1.2.1",
    "react-keydown": "^1.9.7",
    "regenerator-runtime": "^0.12.1",
    "styled-components": "^3.4.2",
    "styled-css-grid": "^0.11.0",
    "threads": "^0.12.0",
    "underscore": "^1.9.1",
    "unissist": "^1.3.0",
    "unistore": "^3.0.6",
    "ytdl-core": "^0.28.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-node-modules": "^0.0.1",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-preact": "^1.1.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "cross-env": "^5.2.0",
    "eslint": "5.7.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-preact": "^1.1.6",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^3.1.0",
    "nodemon": "^1.18.3",
    "npm-run-all": "^4.1.5",
    "parcel": "^1.10.3",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "preact-render-to-string": "^3.8.0"
  }
}
